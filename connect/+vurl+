$(document).ready(function(){
    $("#burger").click(function(){
        $(".nav ul").css("height","auto");
        $(".nav ul").css("padding","50px 0");
    });
    $("#nav-close").click(function(){
        $(".nav ul").removeAttr("style");
    });
});
</script>
