<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?sensor=false"></script>
        
        <script type="text/javascript">
            // When the window has finished loading create our google map below
            google.maps.event.addDomListener(window, 'load', init);
        
            function init() {
                // Basic options for a simple Google Map
                // For more options see: https://developers.google.com/maps/documentation/javascript/reference#MapOptions
                var mapOptions = {
                    // How zoomed in you want the map to start at (always required)
                    zoom: 15,

                    // The latitude and longitude to center the map (always required)
                    center: new google.maps.LatLng(51.6012, -0.2388), // New York

                    // How you would like to style the map. 
                    // This is where you would paste any style found on Snazzy Maps.
                    styles: [{"featureType":"water","elementType":"geometry","stylers":[{"color":"#e9e9e9"},{"lightness":17}]},{"featureType":"landscape","elementType":"geometry","stylers":[{"color":"#f5f5f5"},{"lightness":20}]},{"featureType":"road.highway","elementType":"geometry.fill","stylers":[{"color":"#ffffff"},{"lightness":17}]},{"featureType":"road.highway","elementType":"geometry.stroke","stylers":[{"color":"#ffffff"},{"lightness":29},{"weight":0.2}]},{"featureType":"road.arterial","elementType":"geometry","stylers":[{"color":"#ffffff"},{"lightness":18}]},{"featureType":"road.local","elementType":"geometry","stylers":[{"color":"#ffffff"},{"lightness":16}]},{"featureType":"poi","elementType":"geometry","stylers":[{"color":"#f5f5f5"},{"lightness":21}]},{"featureType":"poi.park","elementType":"geometry","stylers":[{"color":"#dedede"},{"lightness":21}]},{"elementType":"labels.text.stroke","stylers":[{"visibility":"on"},{"color":"#ffffff"},{"lightness":16}]},{"elementType":"labels.text.fill","stylers":[{"saturation":36},{"color":"#333333"},{"lightness":40}]},{"elementType":"labels.icon","stylers":[{"visibility":"off"}]},{"featureType":"transit","elementType":"geometry","stylers":[{"color":"#f2f2f2"},{"lightness":19}]},{"featureType":"administrative","elementType":"geometry.fill","stylers":[{"color":"#fefefe"},{"lightness":20}]},{"featureType":"administrative","elementType":"geometry.stroke","stylers":[{"color":"#fefefe"},{"lightness":17},{"weight":1.2}]}]
                };

                // Get the HTML DOM element that will contain your map 
                // We are using a div with id="map" seen below in the <body>
                var mapElement = document.getElementById('map');

                // Create the Google Map using our element and options defined above
                var map = new google.maps.Map(mapElement, mapOptions);

                // Let's also add a marker while we're at it
                var marker = new google.maps.Marker({
                    position: new google.maps.LatLng(51.6012, -0.2388),
                    map: map,
                    title: 'Grace Mission International'
                });
            }
        </script>
		<script>
            $(document).ready(function(){
                $.ajax({
                    url: 'https://trinity.churchapp.co.uk/embed/smallgroups/json',
    				jsonp: "callback",
                    dataType: 'jsonp',
                    success: function(response){
        				response.forEach(function(group) {
							var syear = ""+group.date_start[0]+group.date_start[1]+group.date_start[2]+group.date_start[3];
							var smonth = ""+group.date_start[5]+group.date_start[6]-1;
							var sday = ""+group.date_start[8]+group.date_start[9];
							var shour = ""+group.time[0]+group.time[1];
							var smin = ""+group.time[3]+group.time[4];
							var sdate = new Date(syear,smonth,sday,shour,smin,00,0);
							
							var eyear = ""+group.date_end[0]+group.date_end[1]+group.date_end[2]+group.date_end[3];
							var emonth = ""+group.date_end[5]+group.date_end[6]-1;
							var eday = ""+group.date_end[8]+group.date_end[9];
							var ehour = ""+group.time[0]+group.time[1];
							var emin = ""+group.time[3]+group.time[4];
							var edate = new Date(eyear,emonth,eday,ehour,emin,00,0);
							
							var signupyear = ""+group.signup_date_start[0]+group.signup_date_start[1]+group.signup_date_start[2]+group.signup_date_start[3];
							var signupmonth = ""+group.signup_date_start[5]+group.signup_date_start[6]-1;
							var signupday = ""+group.signup_date_start[8]+group.signup_date_start[9];
							var signupdate = new Date(signupyear,signupmonth,signupday,00,00,00,0);
							
							var signendyear = ""+group.signup_date_end[0]+group.signup_date_end[1]+group.signup_date_end[2]+group.signup_date_end[3];
							var signendmonth = ""+group.signup_date_end[5]+group.signup_date_end[6]-1;
							var signendday = ""+group.signup_date_end[8]+group.signup_date_end[9];
							var signenddate = new Date(signendyear,signendmonth,signendday,00,00,00,0);
							
							var months = ["Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"];
							var days = ["Sun","Mon","Tue","Wed","Thu","Fri","Sat"];
							var days2 = ["","Monday","Tuesday","Wednesday","Thursday","Friday","Saturday","Sunday"];
							var jsonurl = ""+group.images.original_1000
							var vurl = jsonurl.replace(/\\/g, "");
							
							var hours = sdate.getHours();
  							var minutes = sdate.getMinutes();
  							var ampm = hours >= 12 ? 'pm' : 'am';
  							hours = hours % 12;
  							hours = hours ? hours : 12; // the hour '0' should be '12'
  							minutes = minutes < 10 ? '0'+minutes : minutes;
							var strTime = (minutes == 00) ? ''+hours+ampm:''+hours+'.'+minutes+ampm;
							
							var currentDate = new Date();
							
							var heading = '<h3>'+group.name+'</h3></span>';
							var groupDetails = '<p><span style="text-transform:capitalize;">'+group.frequency+'</span> '+days2[group.day]+'s '+strTime+'</p>';
							var linkOpen = '<div class="groups-over" style="background-image:url('+vurl+');" id="'+group.identifier+'"><a>';
							var weekday = (group.day != null) ? 'on '+days2[group.day]:'';
							
							var location = (group.location.address_name) ? '<br/><img class="groups-icon" src="http://localhost/g-mission/images2017/groups/location-icon.gif" alt="location icon" />'+group.location.address_name:'';
							var postcode = ''+group.location.address;
							var mapPostcode = postcode.replace(" ", "+");
							var gmap = (group.location.address_name) ? '<div class="google-map-div"><iframe width="600" height="450" frameborder="0" style="border:0" src="https://www.google.com/maps/embed/v1/place?key=AIzaSyCSd9y-Fxadk_Em78VBZxAb44itA4YedlY&q=London+'+mapPostcode+'&zoom=15" allowfullscreen></iframe></div>':'';
							
							var allDetails = '<p><img class="groups-icon" src="http://localhost/g-mission/images2017/groups/calendar-icon.gif" alt="calendar icon" />Meets '+group.frequency+' '+weekday+'<br/><img class="groups-icon" src="http://localhost/g-mission/images2017/groups/time-icon.gif" alt="time icon" />'+strTime+location+'<br/><img class="groups-icon" src="http://localhost/g-mission/images2017/groups/runs-icon.gif" alt="runs icon" />Runs '+group.date_start[8]+group.date_start[9]+' '+months[sdate.getMonth()]+' - '+group.date_end[8]+group.date_end[9]+' '+months[edate.getMonth()]+'</p>';
							
							var signupPercent = ''+Math.round(group.no_members*100/group.signup_capacity);
							
							var signupTrue = '<p style="margin-bottom:15px;"><img class="groups-icon" src="http://localhost/g-mission/images2017/groups/signup-icon.gif" alt="signup icon" />Sign up '+group.signup_date_start[8]+group.signup_date_start[9]+' '+months[signupdate.getMonth()]+' - '+group.signup_date_end[8]+group.signup_date_end[9]+' '+months[signenddate.getMonth()]+'</p><div class="percent-bar"><div class="percent-full" style="width:'+signupPercent+'%;"><span>'+signupPercent+'% full</span></div></div><p style="color:#888; margin:10px 0; font-size:13px; text-align:right; border-bottom:1px solid #ccc; padding-bottom:30px;">Current Members '+group.no_members+' / '+group.signup_capacity+'</p><a class="group-signup-link" href="https://trinity.churchapp.co.uk/groups/'+group.identifier+'" target="_blank">Sign Up</a>';
							
							var signupFalse = '<p style="border-top:1px solid #ccc; padding-top:30px; margin-bottom:20px; color:#d73f39;"><img class="groups-icon" src="http://localhost/g-mission/images2017/groups/signup-icon.gif" alt="signup icon" />Sign up now closed</p>';
							
                            var signupDetails = (signenddate.getTime() > currentDate.getTime()) ? ''+signupTrue:''+signupFalse;
							
							var mainContent = '<div class="group-content-hold"><h2>'+group.name+'</h2>'+allDetails+'<p>'+group.description+'</p>'+signupDetails+'<div class="spacer"></div></div><div class="group-image-head" style="background-image:url('+vurl+');"></div>'+gmap+'<div class="spacer"></div>';
							
        					$('#show-groups').append(''+linkOpen+heading+'</a><div class="spacer"></div></div>');
							
							$("#"+group.identifier).click(function(){
        						$("*").removeClass("open");
        						$("#"+group.identifier).addClass("open");
        						$("#show-groups").addClass("open");
        						$("#groups-holder").addClass("open");
        						$('#groups-content').html(''+mainContent);
    						});
    						$("#close-groups").click(function(){$("*").removeClass("open");});
        				});
						$('#show-groups').append('<div class="spacer"></div>');
                    }
                });
            })
        </script>
